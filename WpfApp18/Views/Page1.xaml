<Page x:Class="WpfApp18.Views.Page1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfApp18.Views"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page1">

    <Grid Background="Beige">
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <Button Style="{StaticResource btn2}" Content="Open" Margin="0,20" Command="{Binding StartCommand}"></Button>
            <Button Style="{StaticResource btn2}" Content="Cancel" Command="{Binding CancelCommand}" ></Button>
        </StackPanel>

    </Grid>
</Page>
